{
    "$schema": "https://raw.githubusercontent.com/sumneko/vscode-lua/master/setting/schema.json",
    "runtime": {
        "version": "LuaJIT"
    },
    "workspace": {
        "checkThirdParty": false,
        "library": [
            "$VIMRUNTIME/lua",
            "${3rd}/luv/library",
            "~/AppData/Local/nvim-data/lazy/snacks.nvim/lua"
        ]
    },
    "diagnostics": {
        "globals": [
            "vim",
            "Snacks"
        ]
    }
}